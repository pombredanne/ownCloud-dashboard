      <div class="col-md-3">
        <div class="FilterItemMetricsEvol" data-data-source="its"
             data-metrics="its_changers" data-min="true" data-filter="repos"></div>
      </div>
      <!-- end changers -->

    </div>
    <!-- end its-->

    <!-- scr -->
    <div class="row">
      <div class="col-md-3">
        <div class="FilterItemSummary" data-data-source="scr"
             data-filter="repos"></div>
      </div>

      <div class="col-md-4">
        <div class="FilterItemMetricsEvol" data-data-source="scr"
             data-metrics="scr_submitted,scr_merged,scr_abandoned"
             data-min="true" data-filter="repos" data-frame-time="true"></div>
      </div>

      <div class="col-md-4">
        <div class="FilterItemMetricsEvol" data-data-source="scr"
             data-metrics="scr_new" data-frame-time="true"
             data-start="1362096000" data-min="true" data-filter="repos"></div>
      </div>
    </div>
    <!-- end scr-->

    <!-- irc -->
    <div class="row">
      <div class="col-md-3">
