<div class="row">

  <!-- summaries and charts -->
  <div class="col-md-12">
    <!-- scm -->
    <div class="row">
      <div class="col-md-3">
        <div class="FilterItemSummary" data-data-source="scm"
             data-filter="repos"></div>
      </div>

      <!-- commits -->
      <div class="col-md-3">
        <div class="FilterItemMetricsEvol" data-data-source="scm"
             data-metrics="scm_commits" data-min="true" data-filter="repos"></div>
      </div>
      <!-- end commits -->

      <!-- authors -->
      <div class="col-md-3">
        <div class="FilterItemMetricsEvol" data-data-source="scm"
             data-metrics="scm_authors" data-min="true" data-filter="repos"></div>
      </div>
      <!-- end authors -->

      <!-- files -->
      <div class="col-md-3">
        <div class="FilterItemMetricsEvol" data-data-source="scm"
             data-metrics="scm_files" data-min="true" data-filter="repos"></div>
      </div>
      <!-- end files -->

    </div>
    <!-- end scm -->

    <!-- its -->
    <div class="row">
      <div class="col-md-3">
        <div class="FilterItemSummary" data-data-source="its"
             data-filter="repos"></div>
      </div>

      <!-- closed, opened tickets -->
      <div class="col-md-3">
        <div class="FilterItemMetricsEvol" data-data-source="its"
             data-metrics="its_closed,its_opened" data-min="true"
             data-filter="repos"></div>
      </div>
      <!-- end closed, opened tickets -->

      <!-- closers, openers -->
      <div class="col-md-3">
        <div class="FilterItemMetricsEvol" data-data-source="its"
             data-metrics="its_closers,its_openers" data-min="true"
